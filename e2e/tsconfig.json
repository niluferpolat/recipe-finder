{
  "extends": "../tsconfig.base.json",
"compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "types": ["cypress"],
    "module": "ESNext",            
    "moduleResolution": "Node",     
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts", "cypress.config.ts"]
}
